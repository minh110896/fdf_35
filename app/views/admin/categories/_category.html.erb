<% @categories .each do |category| %>
  <tr>
    <td><%= category.id %></td>
    <td><%= category.name %></td>
    <td><%= category.created_at %></td>
    <td><%= category.updated_at %></td>
    <td>
    <%= link_to admin_category_path(category), method: :delete,
      class: "fa fa-trash-o btn btn-danger", data: {confirm: (t "users.index.sure") } do %>
    <% end %>
    </td>
  </tr>
<% end %>
